CEMTSToid				: INDEX 	: {ciscoEnvMonTemperatureStatusTable}
CEMTSDoid				: MATCH 	: {CEMTSToid} /\.2\./
ciscoEnvMonTemperatureStatusIndex 	: REGSUB	: {CEMTSDoid} /(\d+)\..*/$1/ 
CEMTSVoid				: MATCH         : {CEMTSToid} /\.3\./ 
CEMTToid				: MATCH         : {CEMTSToid} /\.4\./
CEMTSoid				: MATCH         : {CEMTSToid} /\.6\./

ciscoEnvMonTemperatureStatusDescr	: CHAIN         : {CEMTSDoid} {ciscoEnvMonTemperatureStatusTable}
ciscoEnvMonTemperatureStatusValue	: CHAIN         : {CEMTSVoid} {ciscoEnvMonTemperatureStatusTable}
ciscoEnvMonTemperatureThreshold		: CHAIN         : {CEMTToid} {ciscoEnvMonTemperatureStatusTable}
ciscoEnvMonTemperatureStateRaw		: CHAIN         : {CEMTSoid} {ciscoEnvMonTemperatureStatusTable}

ciscoEnvMonTemperatureState		: SWITCH 	: {ciscoEnvMonTemperatureStateRaw} 1=normal,2=warning,3=critical,4=shutdown,5=non present,6=not functioning		


