ifAdminStatTxt	  : SWITCH        : {ifAdminStatNum} 1=up,2=down,3=testing
ifOperStatTxt	  : SWITCH        : {ifOperStatNum} 1=up,2=down,3=testing,4=unknown,5=dormant,6=not present,7=lower layer down
ifAdminOperStat	  : REGSUB        : {ifAdminStatTxt} /(.*)/$1 \/ {ifOperStatTxt}/
ifStatSwitchAlias : SWITCH        : {ifAlias} "^(CDP|PO|LLDP|MONI|Internal)"=on, ".*"=off
#ifStatSwitchAuto  : SWITCH        : {ifDescr} ".*"={ifDescr}
ifStatSwitchAuto  : SWITCH        : {ifDescr} "^Vlan|^Null|^Tunnel|^Loopback|^NVI|^Voice|^Backplane"=on, ".*"=off
ifStatSwitch      : SWITCH        : {ifStatSwitchAlias} 'on'=on,".*"={ifStatSwitchAuto}

ifStatAlarmingOn  : SWITCH        : {ifDescr} ".*"={ifDescr}
ifStatAlarmingOff : SWITCH        : {ifDescr} ".*"={ifDescr}

ifStatSwitch1     : SWITCH        : {ifStatSwitchAlias} 'on'=on,".*"={ifStatSwitchAuto}
ifStatStat1       : WORST         : {ifStatSwitch1} {ifStatAlarmingOn}
ifStatStat2       : BEST          : {ifStatStat1} {ifStatAlarmingOff}
ifStatStat        : BEST          : {ifStatStat2} {ifAdminOperStat}

ifAliasBox        : REGSUB        : {ifAlias} /(\S+.*)/ [$1]/
ifIndex           : INDEX         : {ifDescr}
ifPri             : REGSUB        : {ifIndex} /(\S+.*)/sprintf('%05s', $1).'-'.{ifDescr}/e
