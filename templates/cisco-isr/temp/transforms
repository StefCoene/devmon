TempRowIndex                    : MATCH  : {entPhysicalName} /^Temp: /
TempDescrRaw                    : CHAIN  : {TempRowIndex} {entPhysicalName}
TempDescr                       : REGSUB : {TempDescrRaw} /^Temp: (\S+)/$1/
TempValue                       : CHAIN  : {TempRowIndex} {entSensorValue}
TempStatusRaw                   : CHAIN  : {TempRowIndex} {entSensorStatus}
TempStatus                      : SWITCH : {TempStatusRaw} 1=ok,2=unavailable,3=nonoperational
TempIndexRaw                    : INDEX  : {TempDescr}
TempIndex                       : REGSUB : {TempIndexRaw} /(.*)/Temp#$1/

entSensorThresholdRelationOp    : SWITCH : {entSensorThresholdRelation} 1=<,2=<=,3=>,4=>=,5===,6=!=

TempSensorThresholdIndex1       : REGSUB : {TempRowIndex} /(.*)/$1.1/
TempSensorThresholdValue1       : CHAIN  : {TempSensorThresholdIndex1} {entSensorThresholdValue}
TempSensorThresholdRelation1    : CHAIN  : {TempSensorThresholdIndex1} {entSensorThresholdRelationOp}
TempSensorThresholdIndex2       : REGSUB : {TempRowIndex} /(.*)/$1.2/
TempSensorThresholdValue2tmp    : CHAIN  : {TempSensorThresholdIndex2} {entSensorThresholdValue}
TempSensorThresholdValue2       : REGSUB : {TempSensorThresholdValue2tmp} /^$/U/
TempSensorThresholdRelation2tmp : CHAIN  : {TempSensorThresholdIndex2} {entSensorThresholdRelationOp}
TempSensorThresholdRelation2    : REGSUB : {TempSensorThresholdRelation2tmp} /^$/ne/
TempSensorThresholdIndex3       : REGSUB : {TempRowIndex} /(.*)/$1.3/
TempSensorThresholdValue3tmp    : CHAIN  : {TempSensorThresholdIndex3} {entSensorThresholdValue}
TempSensorThresholdValue3       : REGSUB : {TempSensorThresholdValue3tmp} /^$/U/
TempSensorThresholdRelation3tmp : CHAIN  : {TempSensorThresholdIndex3} {entSensorThresholdRelationOp}
TempSensorThresholdRelation3    : REGSUB : {TempSensorThresholdRelation3tmp} /^$/ne/

ValueIsBelowThreshold1          : EVAL    : eval(eval({TempValue})." ".{TempSensorThresholdRelation1}." ".eval({TempSensorThresholdValue1})) || 0
ValueIsBelowThreshold2          : EVAL    : eval(eval({TempValue})." ".{TempSensorThresholdRelation2}." ".eval({TempSensorThresholdValue2})) || 0
ValueIsBelowThreshold3          : EVAL    : eval(eval({TempValue})." ".{TempSensorThresholdRelation3}." ".eval({TempSensorThresholdValue3})) || 0
